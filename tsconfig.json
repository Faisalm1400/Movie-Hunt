{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/(tabs)/_layout.jsx",
    "app/movies/[id].jsx",
    "app/_layout.jsx",
    "app/(tabs)/index.jsx",
    "app/(tabs)/profile.jsx",
    "app/(tabs)/saved.jsx",
    "app/(tabs)/search.jsx",
    "nativewind-env.d.ts"
, "services/api.js", "components/MovieCard.jsx", "components/SearchBar.jsx"  ]
}